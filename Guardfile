COFFEE  = 'coffeescript'

IROUTES = 'coffee/routes'
OROUTES = 'routes'

IPUBLIC = 'coffee/public'
OPUBLIC = 'public/javascripts'

IAPP    = 'coffee/app'
OAPP    = './'

ITEST   = 'tests/coffee'
OTEST   = 'tests/javascripts'

guard COFFEE, input: IROUTES, output: OROUTES
guard COFFEE, input: IPUBLIC, output: OPUBLIC
guard COFFEE, input: IAPP,    output: OAPP
guard COFFEE, input: ITEST,   output: OTEST